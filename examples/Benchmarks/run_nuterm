ebmc uart_transmit_5.sv --neural-liveness "2**4-tx_cnt"
ebmc uart_transmit_6.sv --neural-liveness "2**4-tx_cnt"
ebmc uart_transmit_7.sv --neural-liveness "2**4-tx_cnt"
ebmc uart_transmit_8.sv --neural-liveness "2**4-tx_cnt"
